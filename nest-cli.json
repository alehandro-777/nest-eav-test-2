{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
    "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      { "include": "./json", "outDir": "dist/src" },
      {
        "include": "./!generated/prisma",
        "outDir": "dist/src",
        "watchAssets": true
      }
    ],
    "watchAssets": true
  }
}
